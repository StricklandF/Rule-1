async function queryServers(servers) {
  const promises = servers.map(server => INSSS(server));
  return await Promise.all(promises);
}

// 要查询的服务器列表
const servers = [
  {
    id: 1,
    server: "server1.example.com",
    port: 443,
    type: "trojan"
  },
  {
    id: 2,
    server: "server2.example.com",
    port: 443,
    type: "v2ray"
  },
  {
    id: 3,
    server: "server3.example.com",
    port: 443,
    type: "shadowsocks"
  }
];

// 并行查询所有服务器
const results = await queryServers(servers);

// 打印结果
console.log(results);
