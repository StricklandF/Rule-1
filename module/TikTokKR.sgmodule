#!name=TikTokvKR
#!desc=解锁
#!system=ios
[Map Local]
# 使用reject-dict以减少开销
^https?:\/\/(tnc|dm)[\w-]+.+?\.com\/.+?\? data="https://raw.githubusercontent.com/Keywos/rule/main/mocks/reject-dict.json"

[MITM]
hostname = %APPEND% *.byteoversea.com, *.tik-tokapi.com, *.tiktokv.com

[Script]
TikTokvKR = type=http-request,pattern=^https?:\/\/.+\.(byteoversea|tiktokv).com\/.+(_region=CN&|&mcc_mnc=4),requires-body=0,script-path=https://raw.githubusercontent.com/Keywos/rule/main/JS/TikTokvKR.js
